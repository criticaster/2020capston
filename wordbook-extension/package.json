{
  "name": "wordbook-extension",
  "version": "1.1.2",
  "description": "A Vue.js web extension",
  "author": "hansol cho",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.13.1",
    "chart.js": "^2.9.3",
    "firebase": "^7.14.3",
    "jquery": "^3.5.1",
    "sweetalert2": "^9.15.1",
    "vee-validate": "^3.3.0",
    "vue": "^2.6.10",
    "vue-axios-cors": "^1.0.1",
    "vue-chartjs": "^3.5.0",
    "vue-good-table": "^2.19.2",
    "vue-router": "^3.0.1",
    "vue-sidebar-menu": "^4.5.3",
    "vue-swal": "^1.0.0",
    "vue-sweetalert2": "^3.0.5",
    "vuefire": "^2.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/runtime-corejs3": "^7.4.0",
    "@types/chrome": "^0.0.74",
    "archiver": "^3.0.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.0",
    "ejs": "^2.6.1",
    "expose-loader": "^0.7.5",
    "file-loader": "^5.0.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.3.10",
    "webpack-extension-reloader": "^1.1.0"
  }
}
